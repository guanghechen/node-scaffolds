{
  "name": "@guanghechen/eslint-config",
  "version": "6.0.0-alpha.1",
  "description": "basic eslint config for node.js/javascript project",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guanghechen/node-scaffolds/tree/@guanghechen/eslint-config@6.0.0-alpha.1",
    "directory": "packages/eslint-config"
  },
  "homepage": "https://github.com/guanghechen/node-scaffolds/tree/@guanghechen/eslint-config@6.0.0-alpha.1/packages/eslint-config#readme",
  "keywords": [
    "eslint",
    "eslint config"
  ],
  "type": "commonjs",
  "main": "./index.cjs",
