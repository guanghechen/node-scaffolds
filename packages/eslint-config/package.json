    }
  },
  "license": "MIT",
  "engines": {
    "node": ">= 18.0.0"
  },
  "files": [
    "eslint.json",
    "index.cjs",
    "index.mjs",
    "package.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "eslint-plugin-import": "^2.30.0"
  },
  "peerDependencies": {
    "eslint": "^8.0.0"
  }
}
