    "@guanghechen/helper-string": "^6.0.0-alpha.1",
    "@guanghechen/invariant": "^6.0.0-alpha.1",
    "@guanghechen/path": "^1.0.0-alpha.14",
    "@guanghechen/reporter": "^1.0.0-alpha.23",
    "command-exists": "^1.2.9",
    "commander": "^11.1.0",
    "execa": "^8.0.1",
    "inquirer": "9.2.12",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9"
  }
}
